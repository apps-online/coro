{
	"name" : "appsonline/coro",
	"description" : "Coro Library Application",
	"keywords" : [
		"coroApplication"
	],
	"homepage" : "https://appsonline.eu/coro-app",
	"require" : {
		"php" : "^5.6 || ^7.0"
	},
	"config" : {
		"optimize-autoloader" : true
	},
	"autoload" : {
		"psr-4" : {
			"Coro\\" : "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": flase,
	"scripts" : {
		"check" : [
			"@cs-check",
			"@test"
		],
		"cs-check" : "phpcs",
		"cs-fix" : "phpcbf",
		"test" : "phpunit --colors=always",
		"test-coverage" : "phpunit --colors=always --coverage-clover clover.xml"
	}
}
